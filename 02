package com.itzhl.operator;

public class OperatorDemo1 {
    public static void main(String[] args) {
        int a =10;
        int b=3;
        System.out.println(a+b);
        System.out.println(a-b);
        System.out.println(a*b);
        System.out.println(a/b);
        //int类型  Z
        System.out.println(a%b);

        System.out.println(3*1.0/2);//1.5
        System.out.println(3/2*1.0);//1.0


    }
}
